        @LDAP_PREFIX@ServiceNotesURL $ @LDAP_PREFIX@ServiceActionURL $ @LDAP_PREFIX@ServiceDisable $
        @LDAP_PREFIX@ServiceCheckcommand $ @LDAP_PREFIX@Servicegroups $
        @LDAP_PREFIX@ServiceDependency $ @LDAP_PREFIX@ServiceDependencyExecutionFailureCriteria $ @LDAP_PREFIX@ServiceDependencyNotificationFailureCriteria $
        @LDAP_PREFIX@ServiceCheckFreshness $ @LDAP_PREFIX@ServiceFreshnessThreshold ) )


