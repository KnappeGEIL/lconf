objectclass ( 1.3.6.1.4.1.26840.144.3.1 NAME '@LDAP_PREFIX@TemplateHost'
    DESC 'Template for a Nagios / Icinga host'
    SUP top
    MAY ( @LDAP_PREFIX@HostCustomvar $ @LDAP_PREFIX@HostContacts $ @LDAP_PREFIX@HostContactgroups $
        @LDAP_PREFIX@HostNotificationOptions $ @LDAP_PREFIX@HostNotificationInterval $ @LDAP_PREFIX@HostNotificationPeriod $ @LDAP_PREFIX@HostNotificationsEnabled $
        @LDAP_PREFIX@HostCheckPeriod $ @LDAP_PREFIX@HostCheckInterval $ @LDAP_PREFIX@HostCheckRetryInterval $ @LDAP_PREFIX@HostCheckMaxCheckAttempts $
